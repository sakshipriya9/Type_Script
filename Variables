var <variable_name> : number = <value>; // type annotation or type signature

let id : number = 5;
let firstName = "Sakshi"; // TS understands firstName will store strings
// firstName = 12; TSC throws error
var lastName : string = "Priya";
// lastName = 13; TSC throws error
console.log(id, firstName);


//  Union of types
let userId : number | string = "26";
userId = 10;


// Any Type in TS

let x : any = 10;
x = "sakshi";
x = false;
console.log(x);
